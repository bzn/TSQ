                pChar = ds_list_find_value(global.playerTrain.train, i);
                show_debug_message("pos"+string(i)+"id"+string(pChar.charID));
            }

            scr_add_hero(heroID);
            
            //show_debug_message("add hero = "+  string(other.heroID));
            
            show_debug_message("========4========")
