        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(!global.msgbox.isActive)
{
    for(var i=0;i&lt;MAX_HERO;i++)
    {
        var heroID = i+1;

        if(scr_is_can_auto_upgrade(heroID))
        {
//            show_debug_message("LV="+string(heroLV));
            // auto upgrade msgbox
            scr_upgrade_msgbox_reset();
            global.nowHeroID = heroID;
            var msg = "";
            if(global.language == "chs")
            { 
                msg = scr_get_upgrade_message_chs(heroID);
            }
            else
            {
                msg = scr_get_upgrade_message(heroID);
            }
            var heroLV = global.heroLevel[heroID-1];            
            global.msgbox.isActive = true;
            global.msgbox.type = MSG_AUTO_UPGRADE;     
            if(global.language == "chs")
            {       
                global.msgbox.msgTitle  = "        升      级";
                global.msgbox.msgStr[0] = "  "+scr_get_hero_name(heroID)+"  的等级提升了";
            }
            else
            {
                global.msgbox.msgTitle  = "        升      級";
                global.msgbox.msgStr[0] = "  "+scr_get_hero_name(heroID)+"  的等級提升了";
            }
            global.msgbox.msgStr[1] = "          "+msg[0];
            global.msgbox.msgStr[2] = "          "+msg[1];
            global.msgbox.msgStr[3] = ""+msg[2];
            global.msgbox.msgStr[4] = "";
            global.msgbox.msgStr[5] = "";
            global.msgbox.msgStr[6] = "";
            if(heroLV == 1)
            {
                global.msgbox.msgLight[0] = true;
                global.msgbox.msgClr[0] = c_white;
                global.msgbox.msgClr[1] = c_dkgray;
                global.msgbox.msgClr[2] = c_dkgray;
            }
            else if(heroLV == 2)
            {
                global.msgbox.msgLight[1] = true;
                global.msgbox.msgClr[0] = c_white;
                global.msgbox.msgClr[1] = c_white;
                global.msgbox.msgClr[2] = c_dkgray;
            }
            else if(heroLV == 3)
            {
                global.msgbox.msgLight[2] = true;
                global.msgbox.msgClr[0] = c_white;
                global.msgbox.msgClr[1] = c_white;
                global.msgbox.msgClr[2] = c_white;
            }
        }
    }
}

if(greenBtn.isClick)
{
    scr_upgrade_msgbox_reset();
    greenBtn.isClick = false;
    isActive = false;
    switch(type)
    {
        case MSG_UNLOCK:           
            
            var product = "";
            if(global.nowHeroID &gt;= 10)
                product = "h"+string(global.nowHeroID);
            else
                product = "h0"+string(global.nowHeroID);
                
            if(os_type == os_ios)
                scr_iap_buy(product);    
            else
                scr_unlock_hero_free(global.nowHeroID);
                   
        break;
        case MSG_UPGRADE:    
        case MSG_AUTO_UPGRADE:
            if(scr_upgrade_hero(global.nowHeroID))
            {   
                global.shine.isShine = true;
                if(global.language == "chs")
                {
                    show_message_async(scr_get_hero_name(global.nowHeroID)+" 的等級提升到 "+string(global.heroLevel[global.nowHeroID-1]));                
                }
                else
                {
                    show_message_async(scr_get_hero_name(global.nowHeroID)+" 的等级提升到 "+string(global.heroLevel[global.nowHeroID-1]));
                }
                show_debug_message("LEVEL +1");                  
            }
            else
            {
                // error msg
                isActive = true;
                type = MSG_GOSHOP;  
                if(global.language == "chs")
                {
                    msgTitle = "     银 币 不 足";
                    msgStr[0] = "";
                    msgStr[1] = "      是否前往商店购买？";
                    msgStr[2] = "";
                    msgStr[3] = "";
                    msgStr[4] = ""; 
                }
                else
                {
                    msgTitle = "     銀 幣 不 足";     
                    msgStr[0] = "";
                    msgStr[1] = "      是否前往商店購買？";
                    msgStr[2] = "";
                    msgStr[3] = "";
                    msgStr[4] = "";    
                }
            }
        break;
        
        case MSG_GOSHOP:
            global.uiShop.isActive = true;
        break;
    }    
}
else if(redBtn.isClick)
{
    redBtn.isClick = false;
    isActive = false;
}

if(!isActive)
{
    x = unactivePosX;
    y = unactivePosY;
}
else
{
    x = activePosX;
    y = activePosY;
}

if(type == MSG_AUTO_UPGRADE)
{
    greenBtn.x = x; 
    greenBtn.y = y+120;
    redBtn.x = 9999; 
    redBtn.y = 9999;
}
else if(type == MSG_WARNING)
{
    redBtn.x = x; 
    redBtn.y = y+120;
    greenBtn.x = 9999; 
    greenBtn.y = 9999;
}
else
{
    greenBtn.x = x+100; 
    greenBtn.y = y+120;
    redBtn.x = x-100; 
    redBtn.y = y+120;
}

if(isTimerAdd)
{
    timer+=5;
    if(timer &gt;= 100)
        isTimerAdd = false
}
else
{
    timer-=5;
    if(timer &lt;= 0)
        isTimerAdd = true;
}

</string>
